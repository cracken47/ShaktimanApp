<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alabhya.Shaktiman">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ConsumerLandingActivity"
            android:parentActivityName=".ConsumerLandingActivity" />
        <activity android:name=".ConsumerSignUpActivity" />
        <activity android:name=".ConsumerLoginActivity" />
        <activity android:name=".ProducerActivity" />
        <activity android:name=".VendorLandingActivity" />
        <activity android:name=".VendorSignupActivity" />
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home"></activity>
    </application>

</manifest>